<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PR_20.PostPage">
    <ContentPage.Content>
        <StackLayout>
                <Label Text="Запрос Post" FontSize="Medium" HorizontalTextAlignment="Center" TextColor="Black"/>
                <Button Text="Отправить запрос" Clicked="OnButtonClicked"/>
                <Label x:Name="Label1" FontSize="Medium" TextColor="Black"/>
                <Button Clicked="OnGetButtonClicked" Text="Запрос Get"/>
                <Button Clicked="OnPutButtonClicked" Text="Запрос Put"/>
            </StackLayout>
    </ContentPage.Content>
</ContentPage>